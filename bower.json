{
  "name": "angular-require-template",
  "version": "0.0.0",
  "authors": [
    "Sagar Ganatra"
  ],
  "description": "A template that defines how angular and require should be used to generate an angular application",
  "license": "MIT",
  "homepage": "index.html",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-ui-router": "~0.2.13",
    "requirejs": "~2.1.16",
    "jquery": "~2.1.3",
    "normalize-scss": "~3.0.2",
    "lumx": "~0.3.20",
    "angular": "~1.3.14",
    "angular-cookies": "~1.3.14",
    "angular-messages": "~1.3.14"
  },
  "install": {
    "path": "vendor",
    "sources": {
      "lumx": {
        "mapping": [
          {
            "bower_components/lumx/dist/scss/settings/_colors.scss": "_colors.scss"
          },
          {
            "bower_components/lumx/dist/lumx.css": "_lumx.scss"
          },
          {
            "bower_components/lumx/dist/lumx.js": "lumx.js"
          }
        ]
      },
      "velocity": [
        "bower_components/velocity/velocity.min.js",
        "bower_components/velocity/velocity.ui.min.js"
      ]
    }
  },
  "devDependencies": {
    "angular-mocks": "~1.3.14"
  }
}
