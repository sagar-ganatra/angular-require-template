{
  "name": "angular-require-template",
  "version": "0.0.0",
  "authors": [
    "Sagar Ganatra"
  ],
  "description": "A template that defines how angular and require should be used to generate an angular application",
  "license": "MIT",
  "homepage": "index.html",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-ui-router": "~0.2.13",
    "requirejs": "~2.1.16",
    "jquery": "~2.1.3",
    "normalize-scss": "~3.0.2",
    "lumx": "~0.3.20",
    "angular": "~1.3.14",
    "angular-cookies": "~1.3.14",
    "angular-messages": "~1.3.14"
  },
  "install": {
    "path": "bower_components",
    "sources": {
      "mdi": {
        "mapping": [
            {   
                ".tmp/mdi/css/materialdesignicons.min.css":
                "materialdesignicons.css"
            },
            {
                ".tmp/mdi/fonts/materialdesignicons-webfont.ttf":                   "../fonts/materialdesignicons-webfont.ttf"
            },
            {
                  ".tmp/mdi/fonts/materialdesignicons-webfont.woff":                   "../fonts/materialdesignicons-webfont.woff"
            },
            {
                  ".tmp/mdi/fonts/materialdesignicons-webfont.woff2":                   "../fonts/materialdesignicons-webfont.woff2"
            }
        ]
      },
      "lumx": [
          ".tmp/lumx/dist/lumx.js",
          ".tmp/lumx/dist/lumx.css",
          ".tmp/lumx/core/scss/settings/_colors.scss"
      ],
      "velocity": [
        ".tmp/velocity/velocity.min.js",
        ".tmp/velocity/velocity.ui.min.js"
      ]
    }
  },
  "devDependencies": {
    "angular-mocks": "~1.3.14"
  }
}
